CREATE TABLE member(
	memberNo int NOT NULL 
	,memberNm varchar2(2000) NOT NULL 
	,attCnt int NOT NULL 
	,telNo varchar2(2000) NOT NULL 
	,lctn varchar2(2000) NOT NULL 
	,gndr varchar2(2000) NOT NULL 
	,age int  NOT NULL 
	,feeBalance int  NOT NULL 
)

